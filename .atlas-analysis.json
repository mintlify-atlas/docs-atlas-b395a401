  "keyFeatures": [
    "Private SOL and SPL token transfers using zero-knowledge proofs",
    "Merkle tree-based commitment scheme with 26-level tree",
    "Groth16 proof verification on-chain",
    "Configurable fee system with deposit and withdrawal fees",
    "Multi-token support with allowlist for SPL tokens",
    "Fully audited by multiple security firms"
  ],
  "publicApiSurface": [
    "initialize - Initialize the privacy pool for SOL",
    "transact - Deposit or withdraw SOL privately",
    "transact_spl - Deposit or withdraw SPL tokens privately",
    "initialize_tree_account_for_spl_token - Initialize privacy pool for a specific SPL token",
    "update_deposit_limit - Update maximum deposit amount for SOL",
    "update_deposit_limit_for_spl_token - Update maximum deposit amount for SPL tokens",
    "update_global_config - Update fee rates and error margins"
  ]
